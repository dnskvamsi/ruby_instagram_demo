<h1>Login </h1>
<%= form_with url: log_in_path do |form| %>
    <%= form.label :email,class:"form-label mt-3" %>
    <%= form.text_field :email, class:"form-control",placeholder: "dev@gmail.com"%>
    <%= form.label :password,class:"form-label" %>
    <%= form.password_field :password,class:"form-control",placeholder: "Password" %>
    <div class="mt-3">
    <%= form.submit "log-in",class: "btn btn-primary"%>
    </div>
<% end %>